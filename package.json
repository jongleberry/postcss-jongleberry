{
  "name": "postcss-jongleberry",
  "description": "My personal bundle of PostCSS middleware",
  "version": "1.0.1",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jongleberry/postcss-jongleberry",
  "dependencies": {
    "cssnano": "3.4.0",
    "postcss-cssnext": "2.4.0",
    "postcss-nested": "1.0.0",
    "rucksack-css": "0.8.5"
  },
  "devDependencies": {
    "babel-eslint": "4.1.8",
    "codecov": "1.0.1",
    "eslint": "1.10.3",
    "eslint-config-jongleberry": "1.2.3",
    "eslint-plugin-react": "3.16.1",
    "eslint-plugin-standard": "1.3.2",
    "ghooks": "1.0.3",
    "mocha": "2.4.5",
    "nyc": "5.5.0",
    "postcss": "5.0.14"
  },
  "scripts": {
    "lint": "eslint --cache lib test",
    "test": "mocha --bail",
    "test-cov": "nyc node_modules/mocha/bin/_mocha && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "css",
    "postcss",
    "css-modules",
    "cssnext",
    "rucksack"
  ],
  "files": [
    "lib"
  ],
  "main": "lib"
}
